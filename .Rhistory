pollutantmean<-function(directory,pollutant,id=1:200){
mylist<-list.files(path=directory,pattern=".csv")
x<-numeric()
for(i in id){
mydata<-read.csv(mylist[i])
x<-c(x,mydata[[pollutant]])
}
mean(x,na.rm=T)
}
